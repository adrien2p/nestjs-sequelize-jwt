{
  "name": "nestjs-sequelize-jwt",
  "version": "3.1.0",
  "description": "Nest + Sequelize + jwt",
  "main": "src/server.ts",
  "keywords": [
    "nest",
    "nest-js",
    "nestjs",
    "sequelize",
    "orm",
    "nodejs",
    "node",
    "typescript",
    "jwt",
    "jsonwebtoken",
    "dotenv"
  ],
  "author": {
    "name": "Adrien de Peretti",
    "email": "adrien.deperetti@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrien2p/nest-js-sequelize-jwt/issues"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -e 'node_modules/**' -e '**/*.d.ts'  -e 'src/**/*.test.ts' --type-check --fix 'src/**/*.ts'",
    "migrate": "ts-node migrate.ts",
    "start": "ts-node src/server.ts --env=NODE_ENV",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lodash": "^4.14.76",
    "@types/reflect-metadata": "0.0.5",
    "@types/sequelize": "^4.0.75",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "jsonwebtoken": "^7.4.3",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "sequelize": "^4.13.0",
    "sequelize-typescript": "^0.5.0",
    "ts-jest": "^22.4.5",
    "typescript": "^2.5.3",
    "umzug": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.31",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-standard": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
